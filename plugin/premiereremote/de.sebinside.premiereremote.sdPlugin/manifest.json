{
	"Name": "PremiereRemote",
	"Version": "0.1.0.0",
	"Author": "sebinside",
	"Actions": [
		{
            "Icon": "imgs/arrowsmaximize-modern-duotone",
            "Name": "Zoom",
            "Controllers": ["Encoder"],
            "Encoder": {
                "layout": "$X1",
				"TriggerDescription": {
					"Rotate": "Zoom",
					"Push": "Reset zoom to 100"
				}
            },
            "States": [
                {
                    "Image": "imgs/arrowsmaximize-modern-duotone",
					"TitleAlignment": "middle",
					"Title": "Zoom"
                }
            ],
            "UUID": "de.sebinside.premiereremote.zoom"
        },
		{
            "Icon": "imgs/arrowsrotatereverse-modern-sharp-duotone-thin",
            "Name": "Rotate",
            "Controllers": ["Encoder"],
            "Encoder": {
                "layout": "$X1",
				"TriggerDescription": {
					"Rotate": "Rotate",
					"Push": "Reset rotation to 0"
				}
            },
            "States": [
                {
                    "Image": "imgs/arrowsrotatereverse-modern-sharp-duotone-thin",
					"TitleAlignment": "middle",
					"Title": "Rotate"
                }
            ],
            "UUID": "de.sebinside.premiereremote.rotate"
        },
		{
            "Icon": "imgs/arrowsleftright-modern-sharp-duotone-thin",
            "Name": "MoveX",
            "Controllers": ["Encoder"],
            "Encoder": {
                "layout": "$X1",
				"TriggerDescription": {
					"Rotate": "MoveX"
				}
            },
            "States": [
                {
                    "Image": "imgs/arrowsleftright-modern-sharp-duotone-thin",
					"TitleAlignment": "middle",
					"Title": "MoveX"
                }
            ],
