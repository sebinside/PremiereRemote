		{
            "Icon": "imgs/arrowsfromline-modern-sharp-duotone-thin",
            "Name": "MoveY",
            "Controllers": ["Encoder"],
            "Encoder": {
                "layout": "$X1",
				"TriggerDescription": {
					"Rotate": "MoveY"
				}
            },
            "States": [
                {
                    "Image": "imgs/arrowsfromline-modern-sharp-duotone-thin",
					"TitleAlignment": "middle",
					"Title": "MoveY"
                }
            ],
            "UUID": "de.sebinside.premiereremote.ymove"
        }
